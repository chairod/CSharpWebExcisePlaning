
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="AppController">
    @*ส่วนหัวของหน้าจอ*@
    <fw-scroll-fix-top>
        <div class="form-row mb-2">
            <div class="col-12">
                <div class="d-flex float-md-left">
                    <fw-execute-button text="ค้นหา Template"
                                       ng-disabled="$settings.isLoading||$settings.formView.signOff||$settings.formView.processStatus!=0@(!ViewBag.CanCreateBudgetRequest?"||true":"")"
                                       css-icon-class="icofont icofont-ui-search"
                                       css-class="btn btn-primary btn-sm mb-1 mb-md-0 mr-1" ng-click="browseTemplate($event)"></fw-execute-button>
                </div>
                <div class="d-flex float-md-right">
                    <fw-execute-button text="@(ViewBag.RequestId==""?"ส่งคำขอ งปม.":"ปรับปรุงคำขอ")"
                                       ng-disabled="$settings.isLoading||$settings.currYear>$settings.formData.FiscalYear||$settings.formView.processStatus!=0||$settings.formView.signOff==true@(!ViewBag.CanCreateBudgetRequest && string.IsNullOrEmpty(ViewBag.RequestId)?"||true":"")"
                                       css-icon-class="icofont icofont-save"
                                       on-loading="$settings.isSaving"
                                       css-class="btn btn-primary btn-sm mb-1 mb-md-0 mr-1" ng-click="submitSave($event)"></fw-execute-button>
                    <fw-execute-button text="ยกเลิกคำขอ" ng-disabled="$settings.isLoading||$settings.formData.RequestId==''||$settings.formView.processStatus!=0||$settings.formView.signOff==true"
                                       css-icon-class="ion-close"
                                       on-loading="$settings.isReject"
                                       css-class="btn btn-danger btn-sm mb-1 mb-md-0 mr-1" ng-click="submitReject($event)"></fw-execute-button>

                    @* SignOff ได้เฉพาะผู้มีสิทธิ์ *@
                    <fw-execute-button text="SignOff คำขอ" ng-disabled="$settings.isLoading@(ViewBag.CanSignOff ? "" : "||true")||$settings.formData.RequestId==''||$settings.formData.RequestType!='1'||$settings.formView.processStatus!=0||$settings.formView.signOff==true"
                                       css-icon-class="ion-checkmark"
                                       ng-if="$settings.formData.RequestType=='1'"
                                       on-loading="$settings.isSignOff"
                                       css-class="btn btn-primary btn-sm mb-1 mb-md-0 mr-1" ng-click="submitSignOff($event)"></fw-execute-button>
                </div>
            </div>
        </div>
    </fw-scroll-fix-top>

    @*ข้อมูลคำขอ*@
    <div class="card card-block borderless-card shadow-sm m-0 mb-2">
        @*บอกประเภทขอคำของบประมาณ (เงินงบประมาณ หรือ เงินนอกงบประมาณ)*@
        <div class="position-absolute f-14 text-white p-2 bg-danger shadow-lg" style="top:0px;right:0px">
            @(ViewBag.RequestType == "START" ? "งบประมาณต้นปี" : "ของบเพิ่มเติม")
        </div>

        <div class="d-flex pb-2 mb-2 border-bottom" ng-if="$settings.formData.RequestId != '' && $settings.formData.RequestType=='1' && $settings.formView.signOff==false">
            <span class="text-danger f-14 animated fadeIn">*** คำขอต้นปี หากรายการค่าใช้จ่ายถูกต้องครบถ้วนแล้วให้กดที่ปุ่ม "SignOff" เพื่อยืนยันคำขอส่งให้กับกรมสรรพสามิต ***</span>
        </div>


        <div class="form-row">
            <div class="form-group col-12 col-md-3">
                <label class="required-field">ปี (พ.ศ.)</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formData.FiscalYear}}" ng-disabled="true||$settings.isLoading" />
            </div>
            <div class="form-group col-12 col-md-3">
                <label class="required-field">หน่วยงาน</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.depName}}" ng-disabled="true||$settings.isLoading" />
            </div>
            <div class="form-group col-12 col-md-6">
                <label>หมายเหตุ/คำอธิบายเพิ่มเติม<fw-validate-error-output error-messages="$settings.formErrors.RemarkText.ErrorMessages" css-class="ml-1"></fw-validate-error-output></label>
                <textarea class="form-control textarea-noresize"
                          ng-model="$settings.formData.RemarkText" title="{{$settings.formData.RemarkText}}"
                          ng-disabled="$settings.isLoading||$settings.formView.processStatus!=0||$settings.formView.signOff==true"
                          style="height:50px;"></textarea>
            </div>
        </div>
        <div class="form-row mb-2 border-bottom">
            <div class="form-group col-12 col-md-3">
                <label>รายการ คชจ.</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formData.Expenses.length|number:0}} รายการ" ng-disabled="true||$settings.isLoading" />
            </div>
            <div class="form-group col-12 col-md-3">
                <label>รวมคำของบประมาณ (บาท)</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formData.Expenses|budgetRequestSummaryAmounts}} บาท" ng-disabled="true||$settings.isLoading" />
            </div>
            <div class="form-group col-12 col-md-3">
                <label class="required-field">ผู้ทำคำขอ</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.requestName}}" title="{{$settings.formView.requestName}}" ng-disabled="true||$settings.isLoading" />
            </div>
            <div class="form-group col-12 col-md-3">
                <label class="required-field">วันที่ส่งคำขอ</label>
                <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.requestDate}}" ng-disabled="true||$settings.isLoading" />
            </div>
        </div>
        @*<div class="form-row">
                <div class="form-group col-12 col-md-3">
                    <label>แผนงาน</label>
                    <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.planName}}" title="{{$settings.formView.planName}}" ng-disabled="true||$settings.isLoading" />
                </div>
                <div class="form-group col-12 col-md-3">
                    <label>ผลผลิต</label>
                    <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.produceName}}" title="{{$settings.formView.produceName}}" ng-disabled="true||$settings.isLoading" />
                </div>
                <div class="form-group col-12 col-md-3">
                    <label>กิจกรรม</label>
                    <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.activityName}}" title="{{$settings.formView.activityName}}" ng-disabled="true||$settings.isLoading" />
                </div>
                <div class="form-group col-12 col-md-3">
                    <label>งบรายจ่าย</label>
                    <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.budgetTypeName}}" title="{{$settings.formView.budgetTypeName}}" ng-disabled="true||$settings.isLoading" />
                </div>
            </div>*@
        <div class="form-row">
            @*<div class="form-group col-12 col-md-3">
                    <label>หมวด คชจ.</label>
                    <input type="text" class="form-control-plaintext border border-warning text-warning pl-2" value="{{$settings.formView.expensesGroupName}}" title="{{$settings.formView.expensesGroupName}}" ng-disabled="true||$settings.isLoading" />
                </div>*@

            @*แสดงตัวแหล่งเงิน (งปม. หรือ เงินนอก งปม.) กรณีเป็นการขอเงินงบเพิ่มเติม*@
            @if (ViewBag.RequestType == "MORE")
            {
                <div class="form-group col-12 col-md-3 mb-0">
                    <label class="d-none d-md-block">&nbsp;<fw-validate-error-output error-messages="$settings.formErrors.BudgetTypeFlag.ErrorMessages" css-class="ml-1"></fw-validate-error-output></label>
                    <md-checkbox ng-model="$settings.formData.BudgetTypeFlag" ng-true-value="1" ng-false-value="2">เงิน งปม.</md-checkbox>
                    <md-checkbox ng-model="$settings.formData.BudgetTypeFlag" ng-true-value="2" ng-false-value="1">เงินนอก งปม.</md-checkbox>
                </div>
                <div class="form-group col-12 col-md-3 mb-0">
                    <label class="required-field">เลขที่หนังสือ<fw-validate-error-output error-messages="$settings.formErrors.RefCode.ErrorMessages" css-class="ml-1"></fw-validate-error-output></label>
                    <input type="text" class="form-control" ng-model="$settings.formData.RefCode" ng-disabled="$settings.isLoading" maxlength="11" placeholder="ระบุเลขที่หนังสืออ้างอิงคำขอเพิ่มเติม" />
                </div>
            }

            @*ปุ่มสำหรับกำหนด Master Template ของคำขอเงินงบประมาณ
                <div class="form-group col-12 col-md-3">
                    <label class="d-none d-md-block">&nbsp;</label>
                    <fw-execute-button text="กำหนด Template หลัก" ng-disabled="$settings.isLoading||$settings.formData.Expenses.length==0||$settings.formView.processStatus!=0||$settings.formView.signOff==true"
                                       css-icon-class="ion-checkmark"
                                       css-class="btn btn-primary btn-sm mb-1 mb-md-0 mr-1"
                                       ng-click="setMasterTemplate()"></fw-execute-button>
                    <span class="text-danger f-12">*** จะแสดงหน้าต่างให้เลือก Template หลักในกรณีที่เลือกใช้ Template คำของบประมาณมากกว่า 1 รายการ ***</span>
                </div>*@
        </div>
    </div>

    @*รายการ คชจ. ที่ต้องการส่งคำขอเงินงบประมาณ*@
    <div class="card card-block borderless-card shadow-sm m-0">
        <div class="row">
            <div class="col-12 col-md-4">
                <fw-validate-error-output error-messages="$settings.formErrors.Expenses.ErrorMessages" css-class="d-block"></fw-validate-error-output>
            </div>
            <div class="col-12 col-md-8">
                <md-checkbox ng-model="$settings.expensesRequestOnly" class="m-0 float-md-right mb-1" ng-disabled="$settings.isLoading||$settings.formData.Expenses.length==0">แสดงเฉพาะหมวดที่มีคำขอ</md-checkbox>
            </div>
        </div>
        <div class="table-responsive">
            <table class="table table-bordered">
                <tr>
                    <th style="width:60px;max-width:60px;min-width:60px;"></th>
                    <th style="width:auto;max-width:160px;" class="text-left">แผนงาน</th>
                    <th style="width:auto;max-width:160px;" class="text-left">ผลผลิต</th>
                    <th style="width:auto;max-width:160px;" class="text-left">กิจกรรม</th>
                    <th style="width:130px;max-width:130px;min-width:130px;" class="text-left">งบรายจ่าย</th>
                    <th style="width:130px;max-width:130px;min-width:130px;" class="text-left">หมวด คชจ.<span class="ml-1" ng-if="$settings.isLoadExpenses"><i class="rotate-refresh ti-reload"></i></span></th>
                </tr>
                <tr ng-if="$settings.formData.Expenses.length == 0">
                    <th colspan="6" class="text-danger text-center animated fadeIn">--- กดที่ปุ่ม "ค้นหา Template" เพื่อทำคำขอ ---</th>
                </tr>
                <tr ng-repeat-start="($groupIndex, groupItem) in $settings.formData.Expenses|privateFilterExpenses:$settings.expensesRequestOnly" class="bg-primary">
                    <td style="width:60px;max-width:60px;min-width:60px;" class="text-center">{{$groupIndex + 1}}</td>
                    <td style="width:130px;max-width:130px;min-width:130px;" class="text-left word-wrap">{{groupItem.GroupBy.PLAN_NAME||'-'}}</td>
                    <td style="width:130px;max-width:130px;min-width:130px;" class="text-left word-wrap">{{groupItem.GroupBy.PRODUCE_NAME||'-'}}</td>
                    <td style="width:130px;max-width:130px;min-width:130px;" class="text-left word-wrap">{{groupItem.GroupBy.ACTIVITY_NAME||'-'}}</td>
                    <td style="width:130px;max-width:130px;min-width:130px;" class="text-left word-wrap">{{groupItem.GroupBy.BUDGET_TYPE_NAME}}</td>
                    <td style="width:130px;max-width:130px;min-width:130px;" class="text-left word-wrap">{{groupItem.GroupBy.EXPENSES_GROUP_NAME}}</td>
                </tr>
                @*รายการค่าใช้จ่ายภายใต้กลุ่ม แผนงาน ผลผลิต กิจกรรม งบรายจ่าย หมวดค่าใช้จ่าย*@
                <tr>
                    <td colspan="6" class="p-0">
                        <table class="table table-striped m-0">
                            <tr class="bg-secondary text-white">
                                <th style="width:60px;min-width:60px;max-width:60px;"></th>
                                <th style="width:auto;min-width:200px;" class="text-left">รายการค่าใช้จ่าย</th>
                                <th style="width:130px;min-width:130px;max-width:130px;" class="text-right word-wrap">คำขอ (บาท)</th>
                                <th style="width:100px;max-width:100px;min-width:100px;"></th>
                            </tr>
                            <tr ng-repeat="($rowIndex, row) in groupItem.Expenses" ng-class="{'text-primary': row.TOTAL_REQUEST_BUDGET>0, 'text-muted': row.TOTAL_REQUEST_BUDGET<=0}">
                                <td style="width:60px;max-width:60px;min-width:60px;" class="text-center">{{$groupIndex + 1}}.{{$rowIndex + 1}}</td>
                                <td style="width:auto;min-width:200px;" class="word-wrap">{{row.EXPENSES_NAME}}</td>
                                <td style="width:150px;max-width:150px;min-width:150px;" class="text-right">{{row.TOTAL_REQUEST_BUDGET|number:2}}</td>
                                <td style="width:100px;max-width:100px;min-width:100px;">
                                    <a href="javascript:void(0)" class="f-w-900 " ng-class="{'text-primary': row.TOTAL_REQUEST_BUDGET>0, 'text-muted': row.TOTAL_REQUEST_BUDGET<=0}" ng-click="setExpensesDetail($event, row)">รายละเอียด<sup><i class="ml-1 ti-new-window"></i></sup></a>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                @*ผลรวมของกลุ่มข้อมูล*@
                <tr class="bg-secondary text-white">
                    <th colspan="5" class="text-right animated fadeIn">รวม</th>
                    <th class="text-right animated fadeIn">{{groupItem.Expenses|fwSimpleSummary:['TOTAL_REQUEST_BUDGET']:2}}</th>
                </tr>
                <tr ng-repeat-end></tr>
            </table>
        </div>
    </div>
</div>

@section Styles{
    @Styles.Render("~/content/select2")
    @Styles.Render("~/content/datepickerrange")
    @Styles.Render("~/content/tags-input")
}
@section Scripts{
    @Scripts.Render("~/bundle/tags-input")
    @Scripts.Render("~/bundle/inputmask")
    @Scripts.Render("~/bundle/select2")
    @Scripts.Render("~/bundle/inputnumber")
    @Scripts.Render("~/bundle/datepickerrange")
    <script type="text/javascript">
        angular.module("leaveApp").controller('SharedController', function ($scope, $fwModalService) {
            $scope.setExpensesDetail = function (event, row) {
                $fwModalService.getModal('@Url.Action("GetExpensesDetailForm", "BudgetRequest")?expensesFormName=' + $.trim(row.FORM_TEMPLATE_NAME), { $row: row || {} }, function ($scope, $controller, $q, $filter, $fwUtils, $row, $mdDialog, $customHttp, $timeout, $fwModalHelperService) {
                    $scope.$settings = {
                        isLoading: true,
                        expensesName: $row.EXPENSES_NAME || '',
                        expensesColumn: {},
                        personnelTypes: @Html.Raw(Json.Encode(@ViewBag.PersonnelTypes)),
                        positions: [],
                        personnelLevels: [],
                        formData: { rows: [] },
                        expensesInfo: $row || {} // รายละเอียดของรายการ คชจ. ที่กำลังจัดการข้อมูล
                    };

                    // โหลดตำแหน่งงานตามรายการ คชจ. (บางรายการ คชจ. ไม่สามารถใช้ตำแหน่งงานได้)
                    $customHttp.formPost('@Url.Action("RetrievePositionAuthorizeByExpensesId", "Helper")', { expensesId: $row.EXPENSES_ID }).then(function (res) {
                        $scope.$settings.positions = res.data || []
                    }, function () { });
                    // โหลดระดับ C ตามรายการ คชจ. (บางรายการ คชจ. ไม่สามารถใช้ระดับ C ได้)
                    $customHttp.formPost('@Url.Action("RetrievePersonnelLeveAuthorizeByExpensesId", "Helper")', { expensesId: $row.EXPENSES_ID }).then(function (res) {
                        $scope.$settings.personnelLevels = res.data || []
                    }, function () { });

                    // ตรวจสอบประเภทของโครงสร้างข้อมูล เป็น Object or Array
                    if ($row.EXPENSES_DESCRIBEs.length == undefined)
                        $scope.$settings.formData.rows = $.extend(true, {}, $row.EXPENSES_DESCRIBEs);
                    else
                        $scope.$settings.formData.rows = $.extend(true, [], $row.EXPENSES_DESCRIBEs);

                    // กำหนดค่าเริ่มต้นในฟอร์ม
                    // มีการระบุข้อมูลอยู่แล้วให้นำไปแสดงผล
                    //if (typeof $row.EXPENSES_DETAILs == 'object')
                    //    $scope.$settings.formData.rows = $row.EXPENSES_DETAILs;

                    // โหลดข้อมูลคอลัมล์ ของรายการ คชจ.
                    $scope.getExpensesColumn = function (expensesFormName) {
                        $scope.$settings.isLoading = true;
                        $customHttp.formGet('@Url.Action("GetExpensesColumn", "BudgetRequest")', {
                            expensesFormName: expensesFormName
                        }).then(function (res) {
                            $scope.$settings.expensesColumn = $fwUtils.parseJson(res.data) || {};
                            // กระจายข้อความไปยัง แต่ล่ะ Controller เพื่อให้ทราบว่ามีการโหลดโครงสร้างข้อมูล
                            // ของรายการ คชจ. เรียบร้อยแล้ว
                            $scope.$broadcast('GetExpensesColumn.loaded', []);
                            $scope.$settings.isLoading = false;
                        }, function () {
                            $scope.$settings.isLoading = false;
                        });
                    };
                    // โหลดโครงสร้างข้อมูลอื่นๆ
                    $scope.getStructureBy = function (filename) {
                        return $q(function (resolve) {
                            $customHttp.formGet('@Url.Action("GetExpensesColumn", "BudgetRequest")', {
                                expensesFormName: filename
                            }).then(function (res) {
                                resolve($fwUtils.parseJson(res.data));
                            }, function () {
                                resolve(null);
                            });
                        });
                    };
                    // จำนวน & ชื่อฟิลด์ จะแตกต่างกันไปขึ้นอยู่กับ แบบฟอร์มของรายการค่าใช้จ่าย
                    $scope.addItem = function () {
                        return $q(function (resolve) {
                            var newItem = $.extend(true, {}, $scope.$settings.expensesColumn);
                            var index = $scope.$settings.formData.rows.push(newItem);
                            $scope.$broadcast('addItem.complete', []);
                            resolve({ $index: index, $newItem: newItem });
                        });
                    };
                    // เมื่อตำแหน่งงานเปลี่ยนแปลง
                    var positionChangedId = null;
                    $scope.positionChanged = function (row) {
                        $timeout.cancel(positionChangedId);
                        positionChangedId = $timeout(function () {
                            var positionId = +('' + row.PositionId).replace(/[^\d]/g, '');
                            var foundItems = $scope.$settings.positions.filter(function (item) { return item.POSITION_ID === positionId; });
                            row.PositionName = foundItems.length > 0 ? foundItems[0].POSITION_NAME : '';
                        }, 300);
                    };
                    // เมื่อระดับ C เปลี่ยนแปลง
                    var personLevelChangedId = null;
                    $scope.personLevelChanged = function (row) {
                        $timeout.cancel(personLevelChangedId);
                        personLevelChangedId = $timeout(function () {
                            var personLevelId = +('' + row.LevelId).replace(/[^\d]/g, '');
                            var foundItems = $scope.$settings.personnelLevels.filter(function (item) { return item.LEVEL_ID === personLevelId; });
                            row.LevelName = foundItems.length > 0 ? foundItems[0].LEVEL_NAME : ''
                        }, 300);
                    };
                    // เมื่อประเภทบุคลากรเปลี่ยนแปลง
                    var personTypeChangedId = null;
                    $scope.personTypeChanged = function (row) {
                        $timeout.cancel(personTypeChangedId);
                        personTypeChangedId = $timeout(function () {
                            var personTypeId = +('' + row.PersonTypeId).replace(/[^\d]/g, '');
                            var foundItems = $scope.$settings.personnelTypes.filter(function (item) { return item.PERSON_TYPE_ID === personTypeId; });
                            row.PersonTypeName = foundItems.length > 0 ? foundItems[0].PERSON_TYPE_NAME : "";
                        }, 300);
                    };
                    // เมื่อหน่วยนับเปลี่ยนแปลง
                    var unitChangedId = null;
                    $scope.unitChanged = function (row) {
                        $timeout.cancel(unitChangedId);
                        unitChangedId = $timeout(function () {
                            var unitId = +('' + row.UnitId).replace(/[^\d]/g, '');
                            var foundItems = $scope.$settings.units.filter(function (item) { return item.UNIT_ID === unitId; });
                            row.UnitName = foundItems.length > 0 ? foundItems[0].UNIT_TEXT : "";
                        }, 300);
                    };
                    // เมื่อรหัสพนักงานเปลี่ยนแปลง
                    var personCodeChangedId = null;
                    $scope.personCodeChanged = function (item) {
                        $timeout.cancel(personCodeChangedId);
                        personCodeChangedId = $timeout(function () {
                            item.PersonId = null;
                            item.PersonName = null;
                            if ($.trim(item.PersonCode).length == 5) {
                                $customHttp.formPost('@Url.Action("RetrievePersonnelByPersonCode", "Helper")', { personCode: item.PersonCode }).then(function (res) {
                                    var personInfo = res.data || {};
                                    item.PersonId = personInfo.PERSON_ID;
                                    item.PersonName = $filter('textFormat')('{0}{1} {2}', personInfo.PREFIX_NAME || '', personInfo.FIRST_NAME || '', personInfo.LAST_NAME || '');
                                    //item.PersonTypeName = personInfo.PERSON_TYPE_NAME || '';
                                    item.PositionName = personInfo.POSITION_NAME || '';
                                    item.LevelName = personInfo.LEVEL_NAME || ''
                                    item.Salary = personInfo.SALARY || 0;
                                });
                            }
                        }, 700);
                    };
